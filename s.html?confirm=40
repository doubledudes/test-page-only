<script type="text/javascript"> 
/* 2020-01-14 */
function addZero(i) {
  if (i < 10) {
    i = "0" + i;
  }
  return i;
}

var dtt = new Date(2020,10,26,12,31,6);

setInterval(function() {
        dtt.setSeconds(dtt.getSeconds() + 1);
        var time = addZero(dtt.getHours()) +':' + addZero(dtt.getMinutes()) + ':' + addZero(dtt.getSeconds());
        document.getElementById('timepress').value = time;
}, 1000);
/* 2020-01-14 */
</script>


